  </div>
  <div class="form-group">
    <%= f.text_field :oem, placeholder: "Manufacturer", class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.text_field :user_name, placeholder: "User's name", class: "form-control" %>
  </div>
  <div class="form-group">
    <%= f.text_field :user_phone, placeholder: "User's phone", class: "form-control" %>
  </div>
  <div class="form-group">
    
  <!-- create new form -->
  
  </div>
  
  <div class="form-group">
