<%= render 'shared/search_test' %>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Name</th>
      <th>Manufacturer</th>
      <th>Supports Papercut</th>
      <th>Supports Coinop</th>
      <th>Supports card reader</th>
    </tr>
  </thead>
  <tbody>
    <%= div_for @copier do |f| %>
    <tr data-link="<%= copier_path(f) %>">
      <td><%= f.name %></td>
      <td><%= f.manufacturer %></td>
      <td><%= f.papercut %></td>
      <td><%= f.coinop %></td>
      <td><%= f.card_reader %></td>
    </tr>
    <% end %>
  </tbody>
</table>




