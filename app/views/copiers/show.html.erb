<h1>Copier details</h1>
<% if @copier %>
  <p><%= @copier.name %></p>
<% end %>
<% if admin_logged_in? %>
  <%= link_to "Delete", copier_path, method: :delete %>
  <p></p>
  <%= link_to "Edit", edit_copier_path %>
<% end %>
<% if flash[:notice] %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>